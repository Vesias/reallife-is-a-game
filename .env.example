# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Authentication
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters
NEXTAUTH_SECRET=your-nextauth-secret-key-32-chars-plus
NEXTAUTH_URL=http://localhost:3000

# Security Configuration
SECURITY_SECRET=your-32-character-security-secret-key
CSRF_SECRET=your-csrf-protection-secret-key-32-plus
SESSION_SECRET=your-session-secret-key-32-characters-plus

# Rate Limiting
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password

# API Configuration
API_VERSION=v1
API_BASE_URL=http://localhost:3000/api

# Environment
NODE_ENV=development
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Monitoring
LOG_LEVEL=info
ENABLE_SECURITY_LOGGING=true

# E2B Code Interpreter
E2B_API_KEY=your-e2b-api-key

# ElevenLabs Voice AI
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Feature Flags
ENABLE_RATE_LIMITING=true
ENABLE_CSRF_PROTECTION=true
ENABLE_SECURITY_HEADERS=true
STRICT_CSP=false