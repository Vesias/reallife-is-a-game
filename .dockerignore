# Docker ignore file\n# Exclude files and directories from Docker build context\n\n# Dependencies\nnode_modules\nnpm-debug.log*\nyarn-debug.log*\nyarn-error.log*\n\n# Next.js build outputs\n.next/\nout/\nbuild/\n\n# Environment files\n.env*\n!.env.example\n\n# Runtime and logs\nlogs\n*.log\npids\n*.pid\n*.seed\n*.pid.lock\n\n# Coverage directory used by tools like istanbul\ncoverage/\n.nyc_output\n\n# Compiled binary addons\nbuild/Release\n\n# Dependency directories\nnode_modules/\njspm_packages/\n\n# TypeScript cache\n*.tsbuildinfo\n\n# Optional npm cache directory\n.npm\n\n# Optional eslint cache\n.eslintcache\n\n# Microbundle cache\n.rpt2_cache/\n.rts2_cache_cjs/\n.rts2_cache_es/\n.rts2_cache_umd/\n\n# Optional REPL history\n.node_repl_history\n\n# Output of 'npm pack'\n*.tgz\n\n# Yarn Integrity file\n.yarn-integrity\n\n# parcel-bundler cache\n.cache\n.parcel-cache\n\n# Next.js cache\n.next/\n.vercel\n\n# Nuxt.js build / generate output\n.nuxt\ndist\n\n# Storybook build outputs\n.out\n.storybook-out\n\n# Temporary folders\ntmp/\ntemp/\n\n# IDE and editor files\n.vscode/\n.idea/\n*.swp\n*.swo\n*~\n\n# OS generated files\n.DS_Store\n.DS_Store?\n._*\n.Spotlight-V100\n.Trashes\nehthumbs.db\nThumbs.db\n\n# Git\n.git\n.gitignore\nREADME.md\n\n# Docker\nDockerfile*\ndocker-compose*\n.dockerignore\n\n# Testing\ncoverage\n.nyc_output\njunit.xml\n\n# CI/CD\n.github/\n.gitlab-ci.yml\n.travis.yml\n.circleci/\n\n# Documentation\ndocs/\n*.md\n!README.md\n\n# Configuration files that shouldn't be in container\n.eslintrc*\n.prettierrc*\njest.config.js\ncypress.json\ncypress/\n\n# Local development files\n.env.local\n.env.development\n.env.test\n.env.production\n\n# Backup files\n*.backup\n*.bak\n*.tmp\n\n# Claude Flow specific\nmemory/\ncoordination/\nclaude-flow*\n.swarm/\n"
  